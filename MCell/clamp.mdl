boxScale = 1

TIME_STEP = 1e-4
ITERATIONS = 1e1

DEFINE_MOLECULES
{
	A { DIFFUSION_CONSTANT_3D = 6E-6 }
}

DEFINE_SURFACE_CLASSES
{
	zeroBC	{ ABSORPTIVE = A     }
	highBC  { CLAMP_CONCENTRATION A, = 1e-5 }
}


INSTANTIATE world OBJECT
{
	outer BOX 
	{
		CORNERS = [0,0,0],[ 10 * boxScale,  boxScale, boxScalex]
		DEFINE_SURFACE_REGIONS
		{
			zeroBC_Surface
			{
				INCLUDE_ELEMENTS =[ LEFT ]
				SURFACE_CLASS = zeroBC
			}
			highBC_Surface
			{
				INCLUDE_ELEMENTS = [ RIGHT ]
				SURFACE_CLASS = highBC
			}	
		}
	}

	solutes RELEASE_SITE
	{
		SHAPE = world.outer
		MOLECULE = A
		CONCENTRATION = 0
	}
}

REACTION_DATA_OUTPUT
{
	STEP = 1e-6
	{ COUNT[ A, world.outer ] } => "A.dat"
}

