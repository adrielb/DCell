INSTANTIATE world OBJECT
{
	outer BOX
	{
		CORNERS = [0,0,0],[dx,dx,dx]
	}

	formation RELEASE_SITE
	{
		SHAPE = world.outer
		MOLECULE = A
		NUMBER_TO_RELEASE = 1E3
	}
}

REACTION_DATA_OUTPUT
{
	STEP = 1e-4
	{ COUNT[ A, world.outer, ESTIMATE_CONC] } => "y.dat"
}
