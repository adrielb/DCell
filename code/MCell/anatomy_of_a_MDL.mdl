VIZ_DATA_OUTPUT
{
	MODE = VOXEL_IMAGE_MODE
	MOLECULE_FILE_PREFIX = "output/vol"
	OBJECT_FILE_PREFIXES {
		my_box = "output/box"
	}
	
	STATE_VALUES {
		A = 10
	}
	ITERATION_LIST = [[0 TO 10 STEP 1]]
	ITERATION_FRAME_DATA {
		EFFECTOR_POSITIONS = [[0 TO 10 STEP 1]]
	}
}


REACTION_DATA_OUTPUT
{
	/*STEP = dt * 1*/
	
	{COUNT[A,WORLD]} => "./output/A.dat"

}

