iterations = 20
dt = 1e-0 
halfbox = 0.5

TIME_STEP = dt
ITERATIONS = iterations
INTERACTION_RADIUS = 0.003
/*
RADIAL_SUBDIVISIONS = // 1024=2^10 default, 4096=2^12 max
RADIAL_DIRECTIONS = // 16384=2^14 default, 131072=2^17 max 
SEED = // 1-3000
EFFECTOR_GRID_DENSITY = //default 1 mm^-2


CHECKPOINT_ITERATIONS = 2
CHECKPOINT_OUTFILE = "output/chkpt1"
*/

PARTITION_X = [[-halfbox TO halfbox STEP halfbox/5]] // units um
PARTITION_Y = [[-halfbox TO halfbox STEP halfbox/5]]
PARTITION_Z = [[-halfbox TO halfbox STEP halfbox/5]]

DEFINE_MOLECULES
{
	A {DIFFUSION_CONSTANT_3D = 6e-6} // cm^2/s
}

my_box BOX 
{
	CORNERS = [-halfbox,-halfbox,-halfbox],[halfbox,halfbox,halfbox]
}

DEFINE_RELEASE_PATTERN my_release {
	DELAY = 0
	RELEASE_INTERVAL = 2
	TRAIN_INTERVAL = 1
	TRAIN_DURATION = 1
	NUMBER_OF_TRAINS
}

my_release_site SPHERICAL_RELEASE_SITE {
	LOCATION = [0,0,0]
	SITE_DIAMETER = 0
	MOLECULE = A
	NUMBER_TO_RELEASE = 4
}

INSTANTIATE world OBJECT 
{
	system OBJECT my_box { }
	my_release_site OBJECT my_release_site { }
}

