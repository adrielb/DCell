time=10
dt = 1e-10
iterations = time / dt
//iterations = 9999
halfbox = 10
r_init = 1e-12
amp_init = 100e-12

INTERACTION_RADIUS = halfbox
/*
PARTITION_X = [[-halfbox TO halfbox STEP halfbox/1]] // units um
PARTITION_Y = [[-halfbox TO halfbox STEP halfbox/1]]
PARTITION_Z = [[-halfbox TO halfbox STEP halfbox/1]]
*/

k1 = 1e15
k2 = 0.001
k3 = 1e4

TIME_STEP = dt
ITERATIONS = iterations

// Units of Diffusion are cm^2/sec
d3d = 6e-12
DEFINE_MOLECULES
{
	R {D_3D = d3d}    // active LEGI-response
	amp {D_3D = 6e-6
		TARGET_ONLY}  // inactive amp
	AMP {D_3D = 6e-6
		TARGET_ONLY}  // active amp
	Ramp {D_3D = d3d} // R-amp complex
	I2AMP {D_3D = d3d} //I2-AMP complex
	I2 {D_3D = d3d}
}

DEFINE_REACTIONS {
	R + amp -> Ramp [k1]
	Ramp -> R + amp [k2]
	Ramp -> R + AMP [k3]
	I2 + AMP -> I2AMP [k1]
	I2AMP -> I2 + AMP [k2]
	I2AMP -> I2 + amp [k3]
}

my_box BOX 
{
	CORNERS = [-halfbox,-halfbox,-halfbox],[halfbox,halfbox,halfbox]
}

INSTANTIATE world OBJECT 
{
	system OBJECT my_box { }
	I2_release RELEASE_SITE
	{
		SHAPE = world.system
		MOLECULE = I2
		CONCENTRATION = r_init
	}
	R_release RELEASE_SITE
	{
		SHAPE = world.system
		MOLECULE = R
		CONCENTRATION = r_init
	}
	amp_release RELEASE_SITE
	{
		SHAPE = world.system
		MOLECULE = amp
		CONCENTRATION = amp_init
	}
	AMP_release RELEASE_SITE
	{
		SHAPE = world.system
		MOLECULE = AMP
		CONCENTRATION = amp_init
	}
}

REACTION_DATA_OUTPUT
{
	STEP = dt * 1
	
	{COUNT[AMP,WORLD], COUNT[R,WORLD],COUNT[I2,WORLD]} => "./output/A.dat"
}

NOTIFICATIONS
{
	ALL_NOTIFICATIONS=ON
}

WARNINGS
{
	ALL_WARNINGS=ERROR
}